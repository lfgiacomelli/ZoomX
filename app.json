{
  "expo": {
    "name": "authlocal",
    "slug": "authlocal",
    "scheme": "authlocal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appIcon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/adaptive-con.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appIcon.png",
        "backgroundColor": "#000"
      }
    },
    "web": {
      "favicon": "./assets/appIcon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
