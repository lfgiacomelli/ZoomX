{
  "expo": {
    "name": "zoomx",
    "slug": "zoomx",
    "scheme": "zoomx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appIcon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "notification": {
      "icon": "./assets/notification_icon.png",
      "color": "#000000"
    },
    "splash": {
      "image": "./assets/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appIcon.png",
        "backgroundColor": "#000"
      },
      "package": "com.smithgg.zoomx"
    },
    "web": {
      "favicon": "./assets/appIcon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-asset",
      "expo-audio",
      "expo-video"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ded4843a-e544-4322-ba40-2a6c2ca01b12"
      }
    }
  }
}
